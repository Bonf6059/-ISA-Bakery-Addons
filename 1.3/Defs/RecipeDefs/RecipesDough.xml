<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>CraftRiceDough</defName>
    <label>craft rice dought</label>
    <description>Make dough from rice.</description>
    <jobString>making rice dough.</jobString>
    <workAmount>300</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawRice</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ISA_Water_Bucket</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <ISA_Dough>3</ISA_Dough>
    </products>
  </RecipeDef>

