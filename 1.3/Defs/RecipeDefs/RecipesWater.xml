<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RecipeDef>
    <defName>CraftEmptyBucket</defName>
    <label>craft empty bucket</label>
    <description>Make a batch of chemfuel by extracting and condensing wood gas from logs.</description>
    <jobString>crafting empty bucket.</jobString>
    <workAmount>250</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ISA_Empty_Bucket>1</ISA_Empty_Bucket>
    </products>
  </RecipeDef>

<RecipeDef>
    <defName>CollectWater</defName>
    <label>collect water</label>
    <description>Make a batch of chemfuel by extracting and condensing wood gas from logs.</description>
    <jobString>collecting water.</jobString>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <workAmount>150</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ISA_Empty_Bucket</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ISA_Empty_Bucket</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <ISA_Water_Bucket>1</ISA_Water_Bucket>
    </products>
  </RecipeDef>

</Defs>
